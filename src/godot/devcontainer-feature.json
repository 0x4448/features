{
    "name": "Godot Engine",
    "id": "godot",
    "version": "1.0.0",
    "description": "Godot Engine - Multi-platform 2D and 3D game engine",
    "options": {
        "version": {
            "type": "string",
            "description": "version to install",
            "default": "latest"
        },
        "dotnet": {
            "type": "boolean",
            "description": "use .NET version",
            "default": false
        },
        "hash": {
            "type": "string",
            "description": "SHA256 hash of package",
            "default": "none"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/dotnet"
    ]
}
