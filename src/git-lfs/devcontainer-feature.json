{
    "name": "Git LFS",
    "id": "git-lfs",
    "version": "1.0.1",
    "description": "Git extension for versioning large files",
    "options": {
        "version": {
            "type": "string",
            "description": "version to install",
            "default": "latest"
        },
        "hash": {
            "type": "string",
            "description": "SHA256 hash of package",
            "default": "none"
        }
    },
    "dependsOn": {
        "ghcr.io/0x4448/features/common-utils:1": {}
    }
}
